        case 1:
          overwriteAll=true;
          break;
        case 2:
          totalBytesWritten+=fin.length();
          int t_percent=(int)((totalBytesWritten*100)/totalFilesSizes);
          totalCopyProgressBar.setValue(t_percent);
          return;
        case 3:
          skipAll=true;
          break;
          case 4:
            break;
      }
