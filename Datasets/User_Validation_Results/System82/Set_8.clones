                else if (hasPrevious())
                {
                    showPrevious();
                }
                else
                {
                    hideFrame();
                }
            }
            
            if (m_cards.remove(card)) // is this card is relevant
            {
                // we need to update index because cards changed
                m_currentCardIndex = m_cards.indexOf(m_currentCard);
                updateActions();
            }
        }

